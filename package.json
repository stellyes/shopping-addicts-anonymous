{
  "name": "s.a.a.",
  "version": "1.0.0",
  "description": "A shopping experience like you've never seen.",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "server": "cd server && npm start",
    "client": "cd client && npm run dev",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "install": "cd server && npm i && cd ../client && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stellyes/shopping-addicts-anonymous.git"
  },
  "keywords": [
    "e-commerce"
  ],
  "author": "Ryan England",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stellyes/shopping-addicts-anonymous/issues"
  },
  "homepage": "https://shopping-addicts-anonymous.onrender.com/",
  "dependencies": {
    "flickity": "^3.0.0",
    "happy-dom": "^9.20.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.0",
    "concurrently": "^8.2.2"
  }
}
